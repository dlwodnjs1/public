import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("입력 > ");
		String inputdata = sc.nextLine();
		
		int num = Integer.parseInt(inputdata);
		if(num<0) {
			System.out.printf("음수");
		} 
		else if(num>0) {
			System.out.printf("양수");
		}
		else {
			System.out.printf("0");
		}
	}
}
