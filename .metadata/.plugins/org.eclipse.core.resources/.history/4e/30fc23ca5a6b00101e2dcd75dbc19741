package sec01.exam01;

public class Member {
	String name;
	String phone;
	String adderss;
	int sal;
	
	Member() {}
	
	public Member(String name, String phone, String adderss, int sal) {
		this.name = name;
		this.phone = phone;
		this.adderss = adderss;
		this.sal = sal;
	}



	public static void main(String[] args) {
		Member member = new Member();
	}
}
