import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("입력 > ");
		String inputdata = sc.nextLine();
		
		if(inputdata<0) {
			System.out.println("음수");
		}
		else if(inputdata>0) {
			System.out.println("양수");
		}
	}
}
