package sec01.ex01;

public class Ex002 {
	public static void main(String[] args) {
		char var1 = 'A';
		System.out.println(var1);
		
		int x1 = 10;
		int x2 = 010;
		int x3 = 0x10;
		
		System.out.println(x1);
		System.out.println(x2);
		System.out.println(x3);
		
		
		if (true) {
			int v1;
			v1 = 400;
			System.out.println(v1);
		}
		
	}
}
